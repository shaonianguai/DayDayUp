    <Style TargetType="{x:Type controls:RibbonContextualTabGroup}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonContextualTabGroup}" />
    <Style TargetType="{x:Type controls:RibbonTabItem}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonTabItem}" />
    <Style TargetType="{x:Type controls:RibbonGroupBox}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonGroupBox}" />
    <Style TargetType="{x:Type controls:RibbonToolBar}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonToolBar}" />
    <Style TargetType="{x:Type controls:RibbonToolBarControlGroup}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonToolBarControlGroup}" />
    <Style TargetType="{x:Type controls:Ribbon}"
           BasedOn="{StaticResource Ribbon.Styles.Ribbon}" />
    <!--<Style TargetType="{x:Type controls:RibbonWindow}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonWindow}" />-->

    <Style TargetType="{x:Type controls:ContextMenu}"
           BasedOn="{StaticResource Ribbon.Styles.ContextMenu}" />
    <!--<Style TargetType="{x:Type controls:MenuItem}"
           BasedOn="{StaticResource Ribbon.Styles.MenuItem}" />-->
    <Style TargetType="{x:Type controls:GroupSeparatorMenuItem}"
           BasedOn="{StaticResource Ribbon.Styles.MenuGroupSeparator}" />
    <Style TargetType="{x:Type controls:RibbonTitleBar}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonTitleBar}" />
    <Style TargetType="{x:Type controls:RibbonTabControl}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonTabControl}" />
    <Style TargetType="{x:Type controls:QuickAccessToolBar}"
           BasedOn="{StaticResource Ribbon.Styles.QuickAccessToolbar}" />

    <Style TargetType="{x:Type controls:RibbonGroupsContainerScrollViewer}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonGroupsContainerScrollViewer}" />

    <Style TargetType="{x:Type controls:ResizeableContentControl}"
           BasedOn="{StaticResource Ribbon.Styles.ResizeableContentControl}" />
    <Style TargetType="{x:Type controls:ApplicationMenu}"
           BasedOn="{StaticResource Ribbon.Styles.ApplicationMenu}" />
