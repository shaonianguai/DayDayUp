        <ResourceDictionary Source="pack://application:,,,/WzUXRibbon;component/Themes/Controls/Button.xaml" />
        <ResourceDictionary Source="pack://application:,,,/WzUXRibbon;component/Themes/Controls/CheckBox.xaml" />
