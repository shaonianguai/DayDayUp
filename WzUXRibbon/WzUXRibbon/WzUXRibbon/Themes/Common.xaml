        <ResourceDictionary Source="pack://application:,,,/WzUXRibbon;component/Themes/Controls/RibbonScrollViewer.xaml" />
        <ResourceDictionary Source="pack://application:,,,/WzUXRibbon;component/Themes/Controls/ResizeableContentControl.xaml" />
        <ResourceDictionary Source="pack://application:,,,/WzUXRibbon;component/Themes/Controls/ScreenTip.xaml" />
        <ResourceDictionary Source="pack://application:,,,/WzUXRibbon;component/Themes/Controls/ApplicationMenu.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <!-- Set default values -->
    <Thickness x:Key="Ribbon.Values.RibbonTabControl.Content.BorderThickness">0</Thickness>

    <!-- Set default styles -->
    <Style TargetType="{x:Type controls:Spinner}"
           BasedOn="{StaticResource Ribbon.Styles.Spinner}" />
    <Style TargetType="{x:Type controls:TwoLineLabel}"
           BasedOn="{StaticResource Ribbon.Styles.TwoLineLabel}" />
    <Style TargetType="{x:Type controls:KeyTip}"
           BasedOn="{StaticResource Ribbon.Styles.KeyTip}" />
    <Style TargetType="{x:Type controls:ScreenTip}"
           BasedOn="{StaticResource Ribbon.Styles.ScreenTip}" />
    <Style TargetType="{x:Type ToolTip}"
           BasedOn="{StaticResource Ribbon.Styles.ToolTip}" />
    <Style TargetType="{x:Type controls:GalleryGroupContainer}"
           BasedOn="{StaticResource Ribbon.Styles.GalleryGroupContainer}" />
    <Style TargetType="{x:Type controls:SeparatorTabItem}"
           BasedOn="{StaticResource Ribbon.Styles.SeparatorTabItem}" />

    <Style TargetType="{x:Type controls:Button}"
           BasedOn="{StaticResource Ribbon.Styles.Button}" />
    <Style TargetType="{x:Type controls:CheckBox}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonCheckBox}" />
    <Style TargetType="{x:Type controls:DropDownButton}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonDropDownButton}" />
    <Style TargetType="{x:Type controls:SplitButton}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonSplitButton}" />
    <Style TargetType="{x:Type controls:ToggleButton}"
           BasedOn="{StaticResource Ribbon.Styles.RibbonToggleButton}" />
    <Style TargetType="{x:Type controls:Gallery}"
