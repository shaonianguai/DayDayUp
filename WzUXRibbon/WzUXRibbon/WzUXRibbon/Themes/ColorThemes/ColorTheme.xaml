<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <!-- COLORS -->

    <!-- Accent colors -->
    <Color x:Key="Fluent.Ribbon.Colors.AccentBase">#FF0078D7</Color>
    <!-- 80% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent80">#CC0078D7</Color>
    <!-- 60% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent60">#990078D7</Color>
    <!-- 40% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent40">#660078D7</Color>
    <!-- 20% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent20">#330078D7</Color>

    <Color x:Key="Fluent.Ribbon.Colors.Highlight">#FF086F9E</Color>

    <!-- Base colors -->
    <Color x:Key="Fluent.Ribbon.Colors.Black">#FF000000</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Black20">#51000000</Color>
    <Color x:Key="Fluent.Ribbon.Colors.White">#FFFFFFFF</Color>
    <Color x:Key="Fluent.Ribbon.Colors.White20">#51FFFFFF</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray1">#FF333333</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray2">#FF7F7F7F</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray3">#FF9D9D9D</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray4">#FFA59F93</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray5">#FFB9B9B9</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray6">#FFCCCCCC</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray7">#FFD8D8D9</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray8">#FFE0E0E0</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray9">#5EC9C9C9</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray10">#FFF7F7F7</Color>

    <Color x:Key="Fluent.Ribbon.Colors.TransparentWhite">#00FFFFFF</Color>
    <Color x:Key="Fluent.Ribbon.Colors.HighTransparentWhite">#17FFFFFF</Color>

    <!-- Foreground -->
    <Color x:Key="Fluent.Ribbon.Colors.IdealForeground">White</Color>
    <Color x:Key="Fluent.Ribbon.Colors.DarkIdealForegroundDisabled">#ADADAD</Color>

    <!-- Misc colors -->
    <Color x:Key="Fluent.Ribbon.Colors.ExtremeHighlight">#FFFFD232</Color>
    <Color x:Key="Fluent.Ribbon.Colors.DarkExtremeHighlight">#FFF29536</Color>

    <!-- END COLORS -->

    <!-- BRUSHES -->

    <!-- Accent brushes -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.AccentBase" Color="{StaticResource Fluent.Ribbon.Colors.AccentBase}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Accent80" Color="{StaticResource Fluent.Ribbon.Colors.Accent80}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Accent60" Color="{StaticResource Fluent.Ribbon.Colors.Accent60}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Accent40" Color="{StaticResource Fluent.Ribbon.Colors.Accent40}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Accent20" Color="{StaticResource Fluent.Ribbon.Colors.Accent20}" options:Freeze="True" />

    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Highlight" Color="{StaticResource Fluent.Ribbon.Colors.Highlight}" options:Freeze="True" />

    <!-- Misc brushes -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.ExtremeHighlight" Color="{StaticResource Fluent.Ribbon.Colors.ExtremeHighlight}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.DarkExtremeHighlight" Color="{StaticResource Fluent.Ribbon.Colors.DarkExtremeHighlight}" options:Freeze="True" />

    <!-- Base brushes -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.White" Color="{StaticResource Fluent.Ribbon.Colors.White}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.White20" Color="{StaticResource Fluent.Ribbon.Colors.White20}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Black" Color="{StaticResource Fluent.Ribbon.Colors.Black}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Black20" Color="{StaticResource Fluent.Ribbon.Colors.Black20}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray1" Color="{StaticResource Fluent.Ribbon.Colors.Gray1}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray2" Color="{StaticResource Fluent.Ribbon.Colors.Gray2}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray3" Color="{StaticResource Fluent.Ribbon.Colors.Gray3}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray4" Color="{StaticResource Fluent.Ribbon.Colors.Gray4}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray5" Color="{StaticResource Fluent.Ribbon.Colors.Gray5}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray6" Color="{StaticResource Fluent.Ribbon.Colors.Gray6}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray7" Color="{StaticResource Fluent.Ribbon.Colors.Gray7}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray8" Color="{StaticResource Fluent.Ribbon.Colors.Gray8}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray9" Color="{StaticResource Fluent.Ribbon.Colors.Gray9}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Gray10" Color="{StaticResource Fluent.Ribbon.Colors.Gray10}" options:Freeze="True" />

    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.TransparentWhite" Color="{StaticResource Fluent.Ribbon.Colors.TransparentWhite}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.HighTransparentWhite" Color="{StaticResource Fluent.Ribbon.Colors.HighTransparentWhite}" options:Freeze="True" />

    <!-- Foreground -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.IdealForeground" Color="{StaticResource Fluent.Ribbon.Colors.IdealForeground}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.IdealForegroundDisabled" Color="{StaticResource Fluent.Ribbon.Colors.IdealForeground}" Opacity="0.4" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.DarkIdealForegroundDisabled" Color="{StaticResource Fluent.Ribbon.Colors.DarkIdealForegroundDisabled}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.LabelText" Color="{StaticResource Fluent.Ribbon.Colors.Black}" options:Freeze="True" />

    <!-- Generic control brushes -->
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Control.Border" Color="{StaticResource Fluent.Ribbon.Colors.Gray6}" options:Freeze="True" />
    <SolidColorBrush x:Key="Fluent.Ribbon.Brushes.Control.Disabled.Border" Color="{StaticResource Fluent.Ribbon.Colors.Gray5}" options:Freeze="True" />

